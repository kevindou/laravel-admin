{
  "name": "jinxing/laravel-admin",
  "description": "This is a laravel background management",
  "keywords": [
    "laravel",
    "admin",
    "laravel-admin"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/myloveGy/laravel-admin/issues",
    "source": "https://github.com/myloveGy/laravel-admin"
  },
  "authors": [
    {
      "name": "jinxing.liu",
      "email": "jinxing.liu@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "src/app"
    },
    "files": [
      "src/helpers/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "App\\Providers\\AdminProvider"
      ]
    }
  }
}